<h1>ユーザーを作成</h1>
<%= form_with(model: @user, local: true) do |form| %>

  <p><%= form.label :name %></p>
  <p><%= form.text_field :name %></p>

  <p><%= form.label :email %></p>
  <p><%= form.text_field :email %></p>

  <p><%= form.label :password %></p>
  <p><%= form.text_field :password %></p>

  <p><%= form.label :password_confirmation %></p>
  <p><%= form.text_field :password_confirmation %></p>

  <%= form.submit %>
<% end %>
